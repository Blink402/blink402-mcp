#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/shillshady/Work/Crypto/blink402/node_modules/.pnpm/vitest@4.0.8_@types+debug@4.1.12_@types+node@20.19.25_@vitest+ui@4.0.8_jiti@2.6.1_lightningcs_my3bpzeafuasgd7yda7jv7jhh4/node_modules/vitest/node_modules:/Users/shillshady/Work/Crypto/blink402/node_modules/.pnpm/vitest@4.0.8_@types+debug@4.1.12_@types+node@20.19.25_@vitest+ui@4.0.8_jiti@2.6.1_lightningcs_my3bpzeafuasgd7yda7jv7jhh4/node_modules:/Users/shillshady/Work/Crypto/blink402/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/shillshady/Work/Crypto/blink402/node_modules/.pnpm/vitest@4.0.8_@types+debug@4.1.12_@types+node@20.19.25_@vitest+ui@4.0.8_jiti@2.6.1_lightningcs_my3bpzeafuasgd7yda7jv7jhh4/node_modules/vitest/node_modules:/Users/shillshady/Work/Crypto/blink402/node_modules/.pnpm/vitest@4.0.8_@types+debug@4.1.12_@types+node@20.19.25_@vitest+ui@4.0.8_jiti@2.6.1_lightningcs_my3bpzeafuasgd7yda7jv7jhh4/node_modules:/Users/shillshady/Work/Crypto/blink402/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../vitest/vitest.mjs" "$@"
else
  exec node  "$basedir/../vitest/vitest.mjs" "$@"
fi
